@article{limaUncoveringTougheningMechanisms2023,
  title = {Uncovering the Toughening Mechanisms of Bonded Joints through Tailored {{CFRP}} Layup},
  author = {Lima, R. A. A. and Tao, R. and Bernasconi, A. and Carboni, M. and Carrere, N. and {Teixeira de Freitas}, S.},
  year = {2023},
