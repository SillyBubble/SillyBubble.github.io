@article{xuBiomimeticTougheningDesign2024,
  title = {Biomimetic Toughening Design of {{3D-printed}} Polymeric Structures: {{Enhancing}} Toughness through Sacrificial Bonds and Hidden Lengths},
  shorttitle = {Biomimetic Toughening Design of {{3D-printed}} Polymeric Structures},
  author = {Xu, Zhiyuan and Tao, Ran and Masania, Kunal and {Teixeira de Freitas}, Sofia},
  year = {2024},
  month = nov,
  journal = {Materials \& Design},
  volume = {247},
  pages = {113361},
  issn = {0264-1275},
  doi = {10.1016/j.matdes.2024.113361},
